{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:vue/essential",
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "linebreak-style":0,
        "no-param-reassign":0,
        "vue/multi-word-component-names":0,
        "import/no-unresolved":0,
        "max-len":0
    }
}
